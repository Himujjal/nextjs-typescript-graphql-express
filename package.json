  },
  "keywords": [
    "nextjs",
    "ssr",
    "react",
    "express.js",
    "typescript",
    "graphql",
    "apollo",
    "jest",
    "storybook",
    "boilerplate",
    "redux"
  ],  
  "author": "Himujjal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Himujjal/nextjs-typescript-graphql-express/issues"
  },
  "homepage": "https://github.com/Himujjal/nextjs-typescript-graphql-express#readme",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@storybook/react": "^5.2.5",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
