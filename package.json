{
  "name": "nextjs-typescript-graphql-express",
  "version": "0.0.1",
  "description": "A Simple Boilerplate for running NextJs + ExpressJS + TypeScript + GraphQL/Apollo with support for Jest and Storybook",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint:ts": "tslint --project .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "gen": "apollo schema:download && apollo client:codegen --endpoint=http://localhost:3100/api --target typescript --localSchemaFile=schema.json --outputFlat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Himujjal/nextjs-typescript-graphql-express.git"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "keywords": [
    "nextjs",
    "ssr",
    "react",
    "express.js",
    "typescript",
    "graphql",
    "apollo",
    "jest",
    "storybook",
    "boilerplate",
    "redux"
  ],
  "author": "Himujjal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Himujjal/nextjs-typescript-graphql-express/issues"
  },
  "homepage": "https://github.com/Himujjal/nextjs-typescript-graphql-express#readme",
  "dependencies": {
    "@types/js-cookie": "^2.2.4",
    "apollo-boost": "^0.4.4",
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-datasource-rest": "^0.6.6",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.9.7",
    "apollo-server-micro": "^2.9.7",
    "apollo-utilities": "^1.3.2",
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "micro-compress": "^1.0.0",
    "micro-cors": "^0.1.1",
    "next": "^9.1.1",
    "next-redux-wrapper": "^4.0.1",
    "react": "^16.10.2",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/react": "^5.2.5",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "^12.11.2",
    "@types/react": "^16.9.9",
    "@types/react-redux": "^7.1.5",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jsdom": "15.2.0",
    "jsdom-global": "3.0.2",
    "prettier": "^1.18.2",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.6.4"
  }
}
